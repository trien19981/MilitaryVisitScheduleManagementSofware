{
  "name": "military/schedule-be",
  "description": "Military Visit Schedule Management - Backend (PHP)",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^8.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.68",
    "phpstan/phpstan": "^1.12"
  },
  "scripts": {
    "cs:fix": "php-cs-fixer fix",
    "cs:check": "php-cs-fixer fix --dry-run --diff",
    "stan": "phpstan analyse src -l 8"
  }
}


